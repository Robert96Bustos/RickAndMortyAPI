---
import Layout from '../layouts/Layout.astro';
// import Launches from '../components/Launches.astro'
// import CardLaunch from '../components/CardLaunch.astro'
import Personaje from '../components/Personaje.astro'

const datos = await fetch('https://rickandmortyapi.com/api/character')
const data = await datos.json()

console.log(data.results)
---

<Layout title="Rick and Morty API.">
	<main class="grid gap-8 sm:grid-cols-2 lg:grid-cols-3">
		{
			data.results.map((personaje: any) => (
				<Personaje
				personaje= {personaje}/>
			))
		}
	<main/>
</Layout>
